    <item rdf:about="https://wiki.nyrds.net/doku.php?id=rpd:acidic&amp;rev=1766089013&amp;do=diff">
        <dc:format>text/html</dc:format>
        <dc:date>2025-12-18T20:16:53+00:00</dc:date>
